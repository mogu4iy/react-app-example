{
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@views/*": ["views/*"],
      "@config/*": ["config/*"],
      "@services/*": ["services/*"],
      "@plugins/*": ["plugins/*"],
      "@utils/*": ["utils/*"],
      "@redux/*": ["redux/*"]
    }
  },
  "include": ["src"]
}
